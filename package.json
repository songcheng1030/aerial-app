    "@trpc/server": "^9.26.2",
    "cookies-next": "^2.0.4",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.0",
    "firebase": "^9.9.1",
    "next": "12.1.4",
    "prism-react-renderer": "^1.3.1",
