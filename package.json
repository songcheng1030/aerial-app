{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "export": "next build && next export",
    "lint": "next lint",
    "lint:write": "next lint --fix",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "e2e": "firebase emulators:exec --only firestore 'jest --config ./jest.e2e.js --detectOpenHandles'",
    "e2e:watch": "firebase emulators:exec --only firestore 'jest --config ./jest.e2e.js --watch'",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest && npm run e2e",
    "storybook": "start-storybook -p 7001",
    "storybook:build": "build-storybook",
    "emulator": "firebase emulators:start",
    "seed": "cross-env NODE_ENV=development pnpm ts-node script/seed.ts",
    "delete": "cross-env NODE_ENV=development pnpm ts-node script/delete.ts"
  },
  "dependencies": {
